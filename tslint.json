    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "semicolon": false,
        "no-implicit-dependencies": false,
        "no-submodule-imports": false
    },
    "linterOptions": {
        "exclude": [
            "lib"
        ]
    }
}
